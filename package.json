{
  "name": "build-busted",
  "version": "0.1.2",
  "description": "Runs busted unit testing for Lua projects",
  "repository": "https://github.com/xpol/atom-build-busted",
  "license": "MIT",
  "main": "lib/busted.js",
  "engines": {
    "atom": "^1.0.0"
  },
  "providedServices": {
    "builder": {
      "versions": {
        "2.0.0": "provideBuilder"
      }
    }
  },
  "devDependencies": {
    "babel-eslint": "^4.1.5",
    "eslint": "^1.10.1",
    "eslint-config-atom-build": "^1.0.0"
  },
  "keywords": [
    "busted",
    "unit-test",
    "lua",
    "productivity"
  ],
  "eslintConfig": {
    "env": {
      "node": true
    },
    "parser": "babel-eslint",
    "extends": "atom-build",
    "rules": {
      "no-unused-vars": [
        2,
        {
          "vars": "all",
          "args": "all"
        }
      ],
      "quotes": [
        2,
        "single"
      ],
      "space-after-keywords": 2,
      "space-before-blocks": 2,
      "space-before-function-paren": [
        2,
        "never"
      ],
      "space-in-parens": [
        2,
        "never"
      ],
      "strict": [
        2,
        "global"
      ]
    }
  },
  "dependencies": {
    "luaparse": "^0.2.0"
  }
}
